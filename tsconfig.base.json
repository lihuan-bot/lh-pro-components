{
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "dist",
    "target": "es2018",
    "module": "esnext",
    "baseUrl": ".",
    "sourceMap": false,
    "moduleResolution": "node",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "removeComments": false,
    "rootDir": ".",
    "types": [],
    "paths": {
      "lh-pro-components/*": ["packages/*"],
      "graphql-compose": ["node_modules/graphql-compose/"]
    }
  }
}
